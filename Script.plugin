[MITM]
hostname = api.m.jd.com, musicpay.kuwo.cn, vip1.kuwo.cn, *.music.163.com

[Script]
# > 京东比价
#http-response ^https?://api\.m\.jd\.com/client\.action\?functionId=(wareBusiness|serverConfig|basicConfig) requires-body=1,script-path=https://raw.githubusercontent.com/yichahucha/surge/master/jd_price.js
http-response ^https?://api\.m\.jd\.com/(client\.action|api)\?functionId=(wareBusiness|serverConfig|basicConfig|lite_wareBusiness|pingou_item) requires-body=1,script-path=https://raw.githubusercontent.com/kan3Git/Script/main/jd_price_lite.js,requires-body=true, timeout=10, tag=京东价格

# > 酷我音乐（可下载 永久VIP 解锁听书看书VIP）（听书看书添加audiobookpay.kuwo.cn, tingshu.kuwo.cn到hostname）
http-response ^https?:\/\/.*\.(kuwo|lrts)\.(cn|me)\/(a\.p|music\.pay|(vip\/(v2|enc)\/(theme|user\/vip))|(EcomResource|(Mobile)?Ad)Serv(er|ice)).* script-path=https://raw.githubusercontent.com/nameking77/Qx/main/rewrite/kw.js,requires-body=true, timeout=10, tag=酷我音乐

# > 网易云音乐
http-request ^http[s]?:\/\/.+music.+(player\/url|playlist|entrance|\/eapi\/search\/).*$ script-path=https://raw.githubusercontent.com/WeiRen0/Scripts/main/wyun.js, requires-body=false, timeout=10, tag=网易云音乐

# > 彩云（解锁SVIP）(buy.itunes.apple.com, biz.caiyunapp.com)
#http-response 
^https?:\/\/biz\.caiyunapp\.com\/v2\/user.+ requires-body=1,script-path=https://raw.githubusercontent.com/o-Il-o/Loon/master/cytq.js

# > Notability（notability.com）
#http-response 
^https?:\/\/notability\.com\/subscriptions requires-body=1,script-path=https://raw.githubusercontent.com/yqc007/QuantumultX/master/NotabilityProCrack.js
